{
    "name": "openeuropa/oe_list_pages",
    "description": "OpenEuropa List Pages.",
    "type": "drupal-module",
    "license": "EUPL-1.2",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3",
        "drupal/core": "^8.9 || ^9.1",
        "drupal/entity_meta_relation": "dev-OEL-77",
        "drupal/facets": "~1.8",
        "drupal/multivalue_form_element": "dev-OEL-160",
        "drupal/search_api": "~1.19",
        "symfony/options-resolver": "~4.0"
    },
    "require-dev": {
        "composer/installers": "~1.11",
        "drupal/core-composer-scaffold": "^8.9 || ^9.1",
        "drupal/config_devel": "~1.2",
        "drupal/drupal-extension": "~4.0",
        "drush/drush": "~10.3",
        "openeuropa/code-review": "~1.6",
        "openeuropa/drupal-core-require-dev": "^8.9 || ^9.1",
        "openeuropa/task-runner-drupal-project-symlink": "^1.0",
        "phpspec/prophecy-phpunit": "^1 || ^2",
        "drupal/address": "~1.9",
        "drupal/extra_field": "^2.1",
        "openeuropa/oe_link_lists": "dev-master",
        "openeuropa/oe_multilingual": "dev-master",
        "openeuropa/open_vocabularies": "dev-OEL-165",
        "drupal/inline_entity_form": "^1.0-rc9"
    },
    "scripts": {
        "post-install-cmd": "./vendor/bin/run drupal:site-setup",
        "post-update-cmd": "./vendor/bin/run drupal:site-setup"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/openeuropa/multivalue_form_element"
        },
        {
            "type": "vcs",
            "url": "https://github.com/openeuropa/entity_meta_relation"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "autoload": {
        "psr-4": {
            "Drupal\\oe_list_pages\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\oe_list_pages\\": "./tests/src",
            "Drupal\\Tests\\oe_link_lists\\": "./build/modules/contrib/oe_link_lists/tests/src",
            "Drupal\\Tests\\oe_list_pages_link_list_source\\": "./modules/oe_list_pages_link_list_source/tests/src"
        }
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "installer-paths": {
            "build/core": ["type:drupal-core"],
            "build/profiles/contrib/{$name}": ["type:drupal-profile"],
            "build/modules/contrib/{$name}": ["type:drupal-module"],
            "build/themes/contrib/{$name}": ["type:drupal-theme"]
        },
        "patches": {},
        "drupal-scaffold": {
            "locations": {
                "web-root": "./build"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
